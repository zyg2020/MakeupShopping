<p id="notice"><%= notice %></p>

<h1>Malls</h1>


<div class="content">
<table >
  <thead>
    <tr>
      <th>Name</th>
      <th>City</th>
      <th>Lat</th>
      <th>Lng</th>
      <th ></th>
    </tr>
  </thead>

  <tbody>
    <% @malls.each do |mall| %>
      <tr>
        <td><%= mall.name %></td>
        <td><%= mall.city %></td>
        <td><%= mall.lat %></td>
        <td><%= mall.lng %></td>
        <td><%= link_to 'Show', mall %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<%= will_paginate %>

<br>

<%= link_to 'New Mall', new_mall_path %>
