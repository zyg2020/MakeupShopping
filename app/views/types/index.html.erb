<p id="notice"><%= notice %></p>

<h1>Types</h1>
<div class="content">
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @types.each do |type| %>
      <tr>
        <td><%= type.name %></td>
        <td><%= link_to 'Show', type %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>


<br>

